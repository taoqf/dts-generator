		"./tests/support/foo/src/index.ts",
		"./tests/typings/dts-generator/dts-generator.d.ts",
		"./tests/unit/all.ts",
		"./tests/unit/bin/dts-generator.ts",
		"./tests/unit/index.ts",
		"./typings/main.d.ts",
		"./node_modules/typescript/lib/typescript.d.ts"
	]
}
